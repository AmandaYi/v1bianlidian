  //查询待付款
  DaiFuKuan() {
    this.DaiFuKuanPage = 1;//待付款当前页码
    this.dixian = 0; //底线清零
    this.noTab = 0;
    this.orderList = this.segmentsArray[0];

    this.http.get(AppConfig.getDebugUrl + '/api/shop/order/queryOrders?tokenId=' + this.tokenId + '&eq_orderStatus=1&page=' + this.DaiFuKuanPage + '&size=10')
